{
    "name": "web-ifc-3d",
    "version": "1.0.0",
    "description": "Experiment with 3d & web-ifc",
    "scripts": {
        "start": "run-p -s -l start:*",
        "start:frontend": "vite",
        "start:typechecker": "npm run typecheck:watch",
        "build": "run-s -s -n build:*",
        "build:frontend": "vite build",
        "eslint": "run-s -s -n eslint:nofix:*",
        "eslint:nofix:frontend": "eslint ./src/**/*.{js,ts,tsx}",
        "eslint:fix": "run-s -s -n eslint:fix:*",
        "eslint:fix:frontend": "eslint --fix ./src/**/*.{js,ts,tsx}",
        "test": "jest",
        "test:watch": "jest --watch",
        "prettier": "run-s -s -n prettier:*",
        "prettier:json": "prettier --write  \"./**/*.json\"",
        "prettier:ts": "prettier --write  \"./**/*.ts\"",
        "prettier:js": "prettier --write  \"./**/*.js\"",
        "prettier:tsx": "prettier --write  \"./**/*.tsx\"",
        "typecheck": "node ./typecheck.js",
        "typecheck:watch": "node ./typecheck_watch.js",
        "ci": "run-s -s -n ci:*",
        "ci:test": "npm run test",
        "ci:eslint": "npm run eslint",
        "ci:typecheck": "npm run typecheck",
        "docker:build": "node ./scripts/buildDocker",
        "docker:push": "node ./scripts/pushDocker",
        "docker:run": "node ./scripts/runDocker",
        "release": "standard-version",
        "release:test": "standard-version --dry-run",
        "release:minor": "standard-version --release-as minor",
        "release:patch": "standard-version --release-as patch",
        "release:major": "standard-version --release-as major",
        "release:next": "standard-version --prerelease next",
        "prepare": "husky install"
    },
    "keywords": [],
    "author": "vegar.ringdal@gmail.com",
    "license": "todo",
    "devDependencies": {
        "@commitlint/cli": "^17.0.3",
        "@commitlint/config-conventional": "^17.0.3",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.3.0",
        "@types/jest": "^28.1.6",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "@types/testing-library__jest-dom": "^5.14.5",
        "@types/three": "^0.143.0",
        "@typescript-eslint/eslint-plugin": "^5.32.0",
        "@typescript-eslint/parser": "^5.32.0",
        "@vitejs/plugin-react": "^2.0.0",
        "autoprefixer": "^10.4.8",
        "bs-logger": "^0.2.6",
        "camera-controls": "^1.36.0",
        "comlink": "^4.3.1",
        "dotenv": "^16.0.1",
        "esbuild": "^0.14.53",
        "esbuild-helpers": "^0.0.9",
        "eslint": "^8.21.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "fflate": "^0.7.3",
        "husky": "^8.0.1",
        "immer": "^9.0.15",
        "jest": "^28.1.3",
        "lodash.memoize": "^4.1.2",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.14",
        "prettier": "^2.7.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.3.0",
        "standard-version": "^9.5.0",
        "stats.js": "^0.17.0",
        "tailwindcss": "^3.1.8",
        "three": "^0.143.0",
        "ts-jest": "^28.0.7",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4",
        "vite": "^3.0.4",
        "web-ifc": "^0.0.35",
        "zustand": "^4.0.0"
    }
}
